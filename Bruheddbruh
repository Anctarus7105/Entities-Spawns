local Creator = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors%20Entity%20Spawner/Source.lua"))()

local entity = Creator.createEntity({
    CustomName = "Barrage", -- Custom name of your entity
    Model = https://github.com/Anctarus7105/Random-Files/blob/main/BarrageNew.rbxm,
    Speed = 370, -- Percentage, 100 = default Rush speed
    DelayTime = 4.5, -- Time before starting cycles (seconds)
    HeightOffset = 0,
    CanKill = true,
    BreakLights = true,
    FlickerLights = {
        true, -- Enabled
        2, -- Time (seconds)
    },
    Cycles = {
        Min = 1,
        Max = 25,
        WaitTime = 3,
    },
    CamShake = {
        true, -- Enabled
        {5, 15, 0.1, 1}, -- Shake values (don't change if you don't know)
        1, -- Shake start distance (from Entity to you)
    },
    Jumpscare = {
        false, -- Enabled ('false' if you don't want jumpscare)
        {
            Image1 = "https://github.com/vct0721/Assets/blob/main/Barrage.png", -- Image1 url
            Image2 = "https://github.com/vct0721/Assets/blob/main/Barrage.png", -- Image2 url
            Shake = false,
            Sound1 = { 
               0, -- SoundId
                { Volume = 1 }, -- Sound properties
            },
            Sound2 = {
               0, -- SoundId
                { Volume = 0 }, -- Sound properties
            },
            Flashing = {
                false, -- Enabled
                Color3.fromRGB(70, 25, 0), -- Color
            },
            Tease = {
                false, -- Enabled ('false' if you don't want tease)
                Min = 1,
                Max = 1,
            },
        },
    },
CustomDialog = {"You died to Barrage..", "When the light flickered, Hide, If its near a bit!", "Its really Rebounding.", "Hope you follow the guide. "},  -- Custom death message (can be as long as you want)
})

------------------------

-- Run the created entity
Creator.runEntity(entity)
